<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.slfortuner.navigationdrawerpos2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Summary"
            android:exported="false" />
        <activity
            android:name=".CartAddItems"
            android:exported="false"
            android:parentActivityName=".ReportFragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProductFragment"
            android:exported="false"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".ReportFragment"
            android:exported="false"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="Instantiatable" />

        <activity
            android:name=".EditProductsActivity"
            android:exported="false"
            android:parentActivityName=".ProductListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProductListActivity"
            android:exported="false"
            android:parentActivityName=".ProductFragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CartInfoActivity"
            android:exported="false"
            android:parentActivityName=".ReportFragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:noHistory="false"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>